@model IEnumerable<PoliziaMunicipaleApp.Models.Anagrafica>

<h2>Anagrafica</h2>
<table class="table">
    <tr>
        <th>Cognome</th>
        <th>Nome</th>
        <th>Indirizzo</th>
        <th>Città</th>
        <th>CAP</th>
        <th>Cod_Fisc</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Cognome</td>
            <td>@item.Nome</td>
            <td>@item.Indirizzo</td>
            <td>@item.Città</td>
            <td>@item.CAP</td>
            <td>@item.Cod_Fisc</td>
        </tr>
    }
</table>
